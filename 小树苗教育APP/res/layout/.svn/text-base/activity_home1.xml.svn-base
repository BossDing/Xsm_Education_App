<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/homebg"
    android:orientation="vertical" >

    <ImageView
        android:id="@+id/id_home_likeStudyImg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="146dp"
        android:layout_marginLeft="140dp"
        android:layout_toRightOf="@+id/id_home_likeLinkImg"
        android:src="@drawable/btbg" />

    <ImageView
        android:id="@+id/id_home_likeLinkImg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/id_home_likeStudyImg"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="43dp"
        android:layout_marginLeft="90dp"
        android:src="@drawable/btbg" />

    <ImageView
        android:id="@+id/id_home_likeSeeImg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/id_home_likeLinkImg"
        android:layout_marginBottom="19dp"
        android:layout_marginRight="36dp"
        android:layout_toLeftOf="@+id/id_home_likeReadImg"
        android:src="@drawable/btbg" />

    <ImageView
        android:id="@+id/id_home_likeSingImg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/id_home_likeSeeImg"
        android:layout_alignLeft="@+id/id_home_likeLinkImg"
        android:layout_marginBottom="60dp"
        android:layout_marginLeft="112dp"
        android:src="@drawable/btbg" />

    <ImageView
        android:id="@+id/id_home_likeReadImg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="26dp"
        android:layout_marginRight="11dp"
        android:src="@drawable/btbg" />

    <ImageView
        android:id="@+id/id_home_likeSocialImg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/id_home_likeSeeImg"
        android:layout_below="@+id/id_home_likeSeeImg"
        android:layout_marginLeft="72dp"
        android:src="@drawable/btbg" />

</RelativeLayout>