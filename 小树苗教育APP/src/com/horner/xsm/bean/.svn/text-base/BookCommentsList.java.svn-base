package com.horner.xsm.bean;

import java.util.ArrayList;

public class BookCommentsList {
public ArrayList<BookComments> bookComments = new ArrayList<BookComments>();
public String commentSum;
public String level;//书籍详情星级
private String name;

public String getLevel() {
	return level;
}
public void setLevel(String level) {
	this.level = level;
}
public ArrayList<BookComments> getBookComments() {
	return bookComments;
}
public void setBookComments(ArrayList<BookComments> bookComments) {
	this.bookComments = bookComments;
}
public String getCommentSum() {
	return commentSum;
}
public void setCommentSum(String commentSum) {
	this.commentSum = commentSum;
}
public String getName() {
	return name;
}
public void setName(String name) {
	this.name = name;
}

}
